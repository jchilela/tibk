}

.titulo{
font-family: verdana,arial,sans-serif;
font-size: 70%;
color:#21341C;
background-color:#D6E8BB;
}

.aaa:link {
    color: RED;
    text-decoration:none;
}

/* visited link */
.aaa:visited{
    font-family:Verdana;
    font-size:22px;
    color: RED;
}

/* mouse over link */
.aaa:hover {
    color: #FFFFFF;
    text-decoration:none;
    font: bold ;
}

/* selected link */
.aaa:active {
    color: RED;
    text-decoration:none;
}


.aa:link {
    color: #afa3a3;
    text-decoration:none;
}

/* visited link */
.aa:visited{
    font-family:Verdana;
    font-size:22px;
    color: #afa3a3;
}

/* mouse over link */
.aa:hover {
    color: #FF0000;
    text-decoration:none;
    font: bold ;
}

/* selected link */
.aa:active {
    color: #FF9C10;
    text-decoration:none;
}

.fortexto{
font-family: arial,helvetica,verdana ;
font-size: 90%;
}

/* unvisited link */
a:link {
    color: WHITE;
    text-decoration:none;
}

/* visited link */
a:visited {
    color: WHITE;
    text-decoration:none;
}

/* mouse over link */
a:hover {
    color: #FF0000;
    text-decoration:none;
    font: bold ;
}

/* selected link */
a:active {
    color: WHITE;
    text-decoration:none;
}
</style>
